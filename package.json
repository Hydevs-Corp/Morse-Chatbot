{
    "name": "lr-chatbot",
    "version": "1.0.0",
    "description": "GraphQL chatbot listener that connects to morse-back API",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "watch": "nodemon --exec ts-node src/index.ts",
        "test-connection": "ts-node src/test-connection.ts",
        "test-history": "ts-node src/test-history.ts",
        "test-queuer": "ts-node src/test-queuer.ts"
    },
    "keywords": [
        "graphql",
        "chatbot",
        "websocket",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@mistralai/mistralai": "^1.7.3",
        "dotenv": "^16.3.1",
        "graphql": "^16.8.1",
        "graphql-ws": "^5.14.2",
        "node-fetch": "^2.7.0",
        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@types/node": "^20.8.10",
        "@types/node-fetch": "^2.6.7",
        "@types/ws": "^8.5.8",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
